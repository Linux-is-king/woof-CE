download() {
    [ -f attr-2.5.1.tar.gz ] || wget -t 3 -T 60 https://download.savannah.gnu.org/releases/attr/attr-2.5.1.tar.gz
build() {
tar -xvf attr-2.5.1.tar.gz
cd attr-2.5.1
./configure --prefix=/usr \
 --disable-static \
 --sysconfdir=/etc \
 --docdir=/usr/share/doc/attr-2.5.1
make
make install
}
